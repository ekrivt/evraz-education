.quantityform_info .qtyuparrow {
	width: 27px;
	bottom: auto;
	height: 30px;
	position: relative;
	overflow: hidden;
	display: inline-block;
	vertical-align: middle;
	line-height: normal;
	font-size: 15px;
	font-family: 'DroidSans';
	font-weight: normal;
	background: #E4E4E4 !important;
	border: 1px solid #d3d3d3;
	color: #555555;
	border-right: 0;
	float: left;
}
.quantityform_info .qtyuparrow:hover, .quantityform_info .qtydownarrow:hover {
	background: #187716 !important;
	border: 1px solid #187716;
	color: #fff;
}
.shopping_dropdown ul li:hover .quantityform_info .qtyuparrow, .shopping_dropdown ul li:hover .quantityform_info .qtydownarrow {
	display: inline-block;
}
}
 @media (max-width: 350px) {
.shopping_dropdown {
	width: 250px;
}
}
